test.fib{
  -- Implement fibonacci sequence
  import lo.

  fib:(integer) => integer.
  fib(0) => 0.
  fib(1) => 1.
  fib(N) => fib(N-1) + fib(N-2).

  fibList:(integer,integer,list[integer]) => list[integer].
  fibList(Curr,Mx,L) =>  L :-  Curr>Mx .
  fibList(Curr,Max,[N1,N2,..L]) => fibList(N1+N2,Max,[N1+N2,N1,N2,..L]).

  main:(integer){}.
  main(Cnt) :-
    logMsg("Fib of \(Cnt) is \(fib(Cnt))").

  -- show "\(fibList(0,4000000,[1,0]))".
}
