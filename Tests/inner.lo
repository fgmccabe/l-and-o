test.inn{
  -- Test inner classes

  basic <~ {
    id:integer.
  }.

  better <~ {
    id:integer.
    inner:basic.
  }.

  outer:better.
  outer<={
    inner:basic.
    inner<={
      id = o.
    }.

    o:integer.
    o = 47.

    id = inner.id.
  }.

  O:better.
  O = outer.

  I:basic.
  I = O.inner.

  assert O.inner.id = 47.
}
