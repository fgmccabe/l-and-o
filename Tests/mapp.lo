test.mapp{
  -- import lo.

  all t ~~ list[t] ::= '[]' | ',..'(t,list[t]).

  all k,v ~~ map[k,v] <~ {}.

  public mapp:all e,f ~~ (list[e],((e)=>f))=>list[f].
  mapp([],_) => [].
  mapp([e,..l],f) => [f(e),..mapp(l,f)].

  public contract all m/1 ~~ mapping[m] <~ {
    (//):all e,f ~~ (m[e],(e)=>f) => m[f].
  }

  implementation mapping[list] <= {
    L//F => mapp(L,F).
  }

  id:(integer)=>integer.
  id(X) => X.

  assert [1,2,3]//id = [1,2,3].
}
