lo.resources{
  import lo.uri.
  import lo.

  public
  getResource:(uri) => list[integer].
  getResource(U) => explode(_get_file(getUriPath(U))).

  public
  putResource:(uri,list[integer]){}.
  putResource(U,Content) :- _put_file(getUriPath(U),implode(Content)).


  public resourcePresent:(uri){}.
  resourcePresent(U) :- _file_present(getUriPath(U)).
}
