location{
  fileLoc <~ {
    merge:(fileLoc)=>fileLoc.
    url:string.
    line:integer.
    column:integer.
    from:integer.
    length:integer.
  }.

  loc:(symbol,integer,integer)<=>fileLoc.
  loc(Fl,Fr,To)..{
    show()::Fr==To => explode(Fl)<>"["<>Fr.show()<>"]".
    show()=>explode(Fl)<>"["<>Fr.show()<>"-"<>To.show()<>"]".
    url()=>explode(Fl).
    extend(Nt) => loc(Fl,Fr,Nt).
    from = Fr.
    to = To.
  }.
}